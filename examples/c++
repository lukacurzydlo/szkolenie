#include <iostream>

// Funkcja obliczająca silnię liczby całkowitej
int obliczSilnie(int n) {
    if (n == 0 || n == 1) {
        return 1;
    } else {
        int wynik = 1;
        for (int i = 2; i <= n; i++) {
            wynik *= i;
        }
        return wynik;
    }
}

int main() {

    int silnia = obliczSilnie(liczba);

    std::cout << "Silnia z " << liczba << " wynosi: " << silnia << std::endl;

    return 0;
}
